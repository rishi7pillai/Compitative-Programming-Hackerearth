#include<bits/stdc++.h>
#define ll long long int
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
using namespace std;
int main()
{
    fast;
    ll t;cin>>t;
    ll a=0,b=7;
    while(t--)
    {
        ll n;cin>>n;
        ll ap=abs(n-a);
        ll bp=abs(n-b);
        if(ap<bp)
        {
            cout<<"A"<<endl;
            a=n;
        }
        else if(ap==bp)
        {
            cout<<"A"<<endl;
            a=n;
        }
        else if(ap>bp)
        {
            cout<<"B"<<endl;
            b=n;
        }

    }
}
