#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        set<char> st;
        cin>>s;
        for(int i=0;i<s.size();i++)
        {
            if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u' || s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U')
              st.insert(s[i]);

        }
        if(st.size()==5)
          cout<<"YES"<<"\n";
        else
          cout<<"NO"<<"\n";

        st.clear();
    }
}
