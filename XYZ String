#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        int i,a=0,b=0,c=0;
        for(i=0;i<s.size();i++)
          {
              if(s[i]=='x')
                 a++;
              if(s[i]=='y')
                 b++;
              if(s[i]=='z')
                 c++;
          }
         if(a>=2 && b>=2 && c>=2)
            cout<<"YES"<<"\n";
         else
            cout<<"NO"<<"\n";
    }
}
