#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
#define ll long long int
#define mod 1e6+7
using namespace std;
int main()
{
    fast;
    ll n;cin>>n;
    if(n>=5)
    {
       ll p=n/5;
       ll m=n%5;
       if(m!=0)
       {
           cout<<p+1<<endl;
       }
       else
         cout<<p<<endl;
    }
    else if(n==0)
       cout<<0<<endl;
    else
      cout<<1<<endl;

}
