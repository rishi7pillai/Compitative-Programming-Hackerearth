#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    getline(cin,s);
    int n,m,i;
    cin>>n>>m;
    if(islower(s[n-1]))
          s[n-1]=toupper(s[n-1]);
    else
       s[n-1]=tolower(s[n-1]);
    
    if(islower(s[m-1]))
        s[m-1]=toupper(s[m-1]);
    else
       s[m-1]=tolower(s[m-1]);
    
    cout<<s;
}
