#include<bits/stdc++.h>
#define ll long long int
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
using namespace std;
int main()
{
    fast;
    string s;cin>>s;
    ll x=0,y=0;
    for(ll i=0;i<s.size();i++)
    {
        if(s[i]=='L')
           x=x-1;
        else if(s[i]=='R')
           x=x+1;
        else if(s[i]=='U')
           y=y+1;
        else if(s[i]=='D')
           y=y-1;
    }
    cout<<x<<" "<<y<<endl;
}
