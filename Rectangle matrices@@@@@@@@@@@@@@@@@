#include<bits/stdc++.h>
using namespace std;
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define ll long long int
#define M 1000000007

int main()
{
    ll t;
    cin>>t;
    while(t--)
    {
        ll a,b;
        cin>>a>>b;
        ll gc=__gcd(a,b);
        if((gc&(gc-1))==0)
           cout<<"Yes\n";
        else
           cout<<"No\n";
        
    }
}
