#include<bits/stdc++.h>
#define ll long long int
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
using namespace std;
int main()
{
    fast;
    ll n;
    cin>>n;
    ll a[n],i,k,p;
    for(i=0;i<n;i++) cin>>a[i];
    cin>>k;
    for(i=0;i<n;i++)
    {
        if(a[i]==k)
        {
            p=i;
            break;

        }
    }
    cout<<i<<endl;
}
