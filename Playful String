#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,i,j;
    cin>>t;
    while(t--)
    {
        string s,sp;
        cin>>s>>sp;
        reverse(sp.begin(),sp.end());
        j=0;
        for(i=0;i<s.size();i++)
        {
            //cout<<s[i]<<" "<<sp[j]<<endl;
            if(s[i]==sp[j])
            {
                j++;
            }
        
        }
       // cout<<j<<endl;
        if(j==sp.size())
           cout<<"GOOD STRING"<<"\n";
        else
           cout<<"BAD STRING"<<"\n";        

         
        

    }
}
