#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n";
#define ll long long int
using namespace std;
int main()
{
    fast;
    ll mod=1e9+7;
    ll n;cin>>n;
    ll a[n],i;
    for(i=0;i<n;i++) cin>>a[i];
    ll ans=1;
    for(i=0;i<n;i++)
        ans=(ans*a[i])%mod;
    cout<<ans<<endl;
    
    
}
