#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s;
    int c=0;
    cin>>s;
    string sp=s;
    reverse(s.begin(),s.end());
    int i;
    for(i=0;i<s.size();i++)
    {
        if(s[i]=='a' ||s[i]=='e' || s[i]=='i' || s[i]=='o' ||s[i]=='u')
        {
            if(sp[i]=='a'||sp[i]=='e'||sp[i]=='i'||sp[i]=='o' ||sp[i]=='u'){
                if(s[i]==sp[i])
                    c++;
            }
        }

    } 
    cout<<c;   
}
