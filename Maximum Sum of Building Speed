#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
#define ll long long int
#define mod 1e6+7
using namespace std;
int main()
{
    fast;
    ll n;cin>>n;
    n=n*2;
    ll a[n],i;
    for(i=0;i<n;i++) cin>>a[i];
    sort(a,a+n);
    ll sum=0;
    for(i=0;i<n;i=i+2)
        sum+=a[i];
    cout<<sum<<endl;

}
