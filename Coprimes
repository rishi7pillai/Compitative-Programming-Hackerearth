#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
#define ll long long int
#define mod 1e6+7
using namespace std;
int main()
{
    fast;
    ll n;cin>>n;
    ll k=0;
    for(ll i=1;i<n-1;i++)
    {
        if(__gcd(i,n)==1)
        {
            k=i;
        }
    }
    cout<<k<<endl;
    
}
