#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
#define ll long long int
#define mod 1e6+7
using namespace std;
int main()
{
    fast;
    ll n;cin>>n;
    string s;cin>>s;
    ll i;
    set<string ,greater<string>> st;
    for(i=0;i<n;i++)
      st.insert(s.substr(i));
    cout<<*(st.begin())<<endl;
    
}
