#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
#define ll long long int
#define mod 1e6+7
using namespace std;
int main()
{
    fast;
    ll n;cin>>n;
    ll a[n],i,x=0;
    for(i=0;i<n;i++)
    {
        cin>>a[i];
        x^=a[i];
    }
    if(x==0)
      {cout<<-1<<endl;}
    else
      cout<<1<<endl;
}
