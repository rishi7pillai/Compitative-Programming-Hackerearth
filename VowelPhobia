#include<bits/stdc++.h>
#include <iostream>
using namespace std;
int main() {
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	int n,p=0,i;
	cin>>n;
	while(n--)
	{
		string s;
		p=0;
		cin>>s;
		for(i=0;i<s.size();i++)
		{
			if(s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u')
			{
				p++;
			}
		}
		 
		cout<<p<<"\n";
	}
}
