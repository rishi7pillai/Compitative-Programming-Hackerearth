#include<bits/stdc++.h>
#define fast ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl "\n"
#define ll long long int
#define mod 1e6+7
using namespace std;
int main()
{
    fast;
    string s;cin>>s;
    ll k=0;
    for(ll i=0;i<s.size();i++)
    {
        k+=(s[i]-'a')+1;
    }
    cout<<k<<endl;

}
