#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    cin.ignore();
    while(t--)
    {
        string s;
        cin>>s;
        int i;
         for(i=0;i<s.size();i++)
        {
            if(s[i]=='A' || s[i]=='E' || s[i]=='I' || s[i]=='O' || s[i]=='U' || s[i]=='Y' || s[i]=='a' || s[i]=='e' || s[i]=='i' || s[i]=='o' || s[i]=='u' || s[i]=='y')
              {
                  //cout<<s[i];
              }
              else
              {
                  cout<<'.';
                  if(isupper(s[i]))
                     cout<<(char)tolower(s[i]);
                  else
                     cout<<s[i];
              }
        }
        cout<<"\n";
       
    }
}
